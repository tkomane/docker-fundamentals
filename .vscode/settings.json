{
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "diffEditor.ignoreTrimWhitespace": false,
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "git.autofetch": true,
    "[markdown]": {
        "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
    },
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "editor.fontFamily": "Fira Code",
    "editor.renderWhitespace": "none",
    "editor.trimAutoWhitespace": true,
    "terminal.integrated.fontFamily": "monospace",
    "editor.fontLigatures": true,
    "editor.fontWeight": "450",
    "editor.fontSize": 13,
    "editor.letterSpacing": 1.2,
    "editor.guides.bracketPairs": "active",
    "editor.bracketPairColorization.enabled": false,
    "editor.wordWrap": "on",
    "redhat.telemetry.enabled": true,
    "[yaml]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[shellscript]": {
        "editor.defaultFormatter": "foxundermoon.shell-format"
    },
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "vsicons.dontShowNewVersionMessage": true,
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "mssql.connections": [
        {
            "server": "{{put-server-name-here}}",
            "database": "{{put-database-name-here}}",
            "user": "{{put-username-here}}",
            "password": ""
        }
    ],
    "yaml.schemas": {
        "file:///c%3A/Users/tkomane/.vscode/extensions/atlassian.atlascode-2.10.7/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "atlascode.jira.enabled": false,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "files.autoSave": "afterDelay",
    "security.workspace.trust.untrustedFiles": "open",
    "window.zoomLevel": 1,
    "window.autoDetectColorScheme": true,
    "workbench.productIconTheme": "material-product-icons",
    "workbench.iconTheme": "macos-modern-big-sur-icon-theme",
